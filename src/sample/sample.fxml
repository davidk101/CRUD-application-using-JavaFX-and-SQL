<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="sample.Controller">
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <AnchorPane prefHeight="456.0" prefWidth="717.0">
      <VBox layoutX="459.0" layoutY="39.0" prefHeight="164.0" prefWidth="58.0" spacing="25.0">
         <Label text="ID" />
         <Label text="Year" />
         <Label text="Make" />
         <Label text="Model" />
      </VBox>
      <VBox layoutX="525.0" layoutY="39.0" prefHeight="164.0" prefWidth="147.0" spacing="13.0">
         <TextField fx:id="id_text" />
         <TextField fx:id="year_text" />
         <TextField fx:id="make_text" />
         <TextField fx:id="model_text" />
      </VBox>
      <TableView fx:id="main_table" layoutX="31.0" layoutY="35.0" prefHeight="387.0" prefWidth="401.0">
         <columns>
            <TableColumn fx:id="id_column" prefWidth="100.0" text="ID" />
            <TableColumn fx:id="year_column" prefWidth="100.0" text="Year" />
            <TableColumn fx:id="make_column" prefWidth="100.0" text="Make" />
            <TableColumn fx:id="model_column" prefWidth="100.0" text="Model" />
         </columns>
      </TableView>
      <Button fx:id="create_btn" layoutX="525.0" layoutY="246.0" mnemonicParsing="false" text="Create" onAction="#buttonPressed"/>
      <Button fx:id="update_btn" layoutX="525.0" layoutY="302.0" mnemonicParsing="false" text="Update" />
      <Button fx:id="delete_btn" layoutX="526.0" layoutY="359.0" mnemonicParsing="false" text="Delete" />
   </AnchorPane>
</GridPane>
